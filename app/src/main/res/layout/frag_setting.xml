<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.aunthtoo.timetableforcupyay.fragments.FragSetting">

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/urdetail"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_marginTop="10dp"
                    android:padding="8dp"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="1dp"
                    app:cardUseCompatPadding="true">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginTop="10dp">

                        <me.myatminsoe.mdetect.MMTextView
                            android:id="@+id/detailtitle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="left"
                            android:padding="2dp"
                            android:text="@string/urdetail"
                            android:textColor="@color/colorAccent"
                            android:textSize="17sp" />

                        <View
                            android:id="@+id/detaildivider"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@id/detailtitle"
                            android:layout_marginTop="3dp"
                            android:background="@color/colorAccent" />

                        <!--start of roll number-->
                        <LinearLayout
                            android:id="@+id/detaildivider1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/detaildivider"
                            android:layout_marginTop="@dimen/detailtopmargin"
                            android:paddingLeft="@dimen/detailpadding"
                            android:paddingRight="@dimen/detailpadding"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:padding="2dp"
                                android:text="@string/urroll"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:id="@+id/yourroll"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="2dp"
                                android:text="52"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <!--end of roll number-->

                        <!--start of your name-->
                        <LinearLayout
                            android:id="@+id/detaildivider2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/detaildivider1"
                            android:layout_marginTop="@dimen/detailmargin"
                            android:paddingLeft="@dimen/detailpadding"
                            android:paddingRight="@dimen/detailpadding"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:padding="2dp"
                                android:text="@string/urname"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:id="@+id/yourname"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="2dp"
                                android:text="Aunk Htoo"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <!--end of your name-->

                        <!--start of your ph no-->
                        <LinearLayout
                            android:id="@+id/detaildivider3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/detaildivider2"
                            android:layout_marginTop="@dimen/detailmargin"
                            android:paddingLeft="@dimen/detailpadding"
                            android:paddingRight="@dimen/detailpadding"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:padding="2dp"
                                android:text="@string/urphno"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:id="@+id/yourph"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="2dp"
                                android:text="09458676496"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <!--end of your ph no-->


                        <!--start of your section-->
                        <LinearLayout
                            android:id="@+id/detaildivider4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/detaildivider3"
                            android:layout_marginTop="@dimen/detailmargin"
                            android:paddingLeft="@dimen/detailpadding"
                            android:paddingRight="@dimen/detailpadding"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:padding="2dp"
                                android:text="@string/section"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:id="@+id/yoursection"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="2dp"
                                android:text="B"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <!--end of your name-->

                        <!--start of edit-->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/detaildivider4"
                            android:layout_marginTop="@dimen/detailtopmargin"
                            android:paddingLeft="@dimen/detailpadding"
                            android:paddingRight="@dimen/detailpadding"
                            android:weightSum="2">

                            <LinearLayout
                                android:id="@+id/logoutbtn"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dp"
                                android:layout_weight="1"
                                android:background="@drawable/greenbgforpresent"
                                android:padding="3dp"
                                android:visibility="invisible"
                                android:weightSum="3">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="20dp"
                                    android:layout_weight="2"
                                    android:src="@drawable/ic_signout" />

                                <me.myatminsoe.mdetect.MMTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:padding="2dp"
                                    android:text="@string/signout"
                                    android:textColor="@android:color/white"
                                    android:textSize="14sp" />

                            </LinearLayout>


                            <LinearLayout
                                android:id="@+id/updatebtn"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dp"
                                android:layout_weight="1"
                                android:background="@drawable/time_bg_green"
                                android:padding="3dp"
                                android:weightSum="3">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="20dp"
                                    android:layout_weight="2"
                                    android:src="@drawable/ic_edit" />

                                <me.myatminsoe.mdetect.MMTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:padding="2dp"
                                    android:text="@string/edit"
                                    android:textColor="@color/detailtextcolor"
                                    android:textSize="14sp" />

                            </LinearLayout>

                        </LinearLayout>


                        <!--end of edit-->

                    </RelativeLayout>

                </android.support.v7.widget.CardView>

            </RelativeLayout>


            <!--about-->
            <RelativeLayout
                android:id="@+id/about"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/urdetail">

                <android.support.v7.widget.CardView
                    android:id="@+id/card"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_marginTop="5dp"
                    android:padding="8dp"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="1dp"
                    app:cardUseCompatPadding="true">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginTop="10dp">


                        <me.myatminsoe.mdetect.MMTextView
                            android:id="@+id/abouttitle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="left"
                            android:padding="2dp"
                            android:text="@string/aboutapp"
                            android:textColor="@color/colorAccent"
                            android:textSize="17sp" />

                        <View
                            android:id="@+id/aboutdivider"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@id/abouttitle"
                            android:layout_marginTop="3dp"
                            android:background="@color/colorAccent" />

                        <LinearLayout
                            android:id="@+id/logodes"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/aboutdivider"
                            android:layout_margin="5dp"
                            android:layout_marginTop="20dp"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/logodesign"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/zarni"
                                android:textSize="14sp"
                                android:textStyle="bold" />

                        </LinearLayout>

                        <View
                            android:id="@+id/logodesdivider"
                            android:layout_width="match_parent"
                            android:layout_height="0.6dp"
                            android:layout_below="@id/logodes"
                            android:layout_marginTop="5dp"
                            android:background="@color/detailtextcolor" />
                        <!--logo designer-->

                        <!--special thank to-->

                        <LinearLayout
                            android:id="@+id/special"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/logodesdivider"
                            android:layout_margin="5dp"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/specialthank"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/myfri"
                                android:textSize="14sp"
                                android:textStyle="bold" />

                        </LinearLayout>

                        <View
                            android:id="@+id/specialdivider"
                            android:layout_width="match_parent"
                            android:layout_height="0.6dp"
                            android:layout_below="@id/special"
                            android:layout_marginTop="5dp"
                            android:background="@color/detailtextcolor" />

                        <!--special thank to-->

                        <!--credit-->

                        <LinearLayout
                            android:id="@+id/credit"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/specialdivider"
                            android:layout_margin="5dp"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/credit"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/justmyacknowledge"
                                android:textSize="14sp"
                                android:textStyle="bold" />

                        </LinearLayout>

                        <View
                            android:id="@+id/creditdivider"
                            android:layout_width="match_parent"
                            android:layout_height="0.6dp"
                            android:layout_below="@id/credit"
                            android:layout_marginTop="5dp"
                            android:background="@color/detailtextcolor" />

                        <!--credit-->

                        <!--feedback-->

                        <LinearLayout
                            android:id="@+id/feedback"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/creditdivider"
                            android:layout_margin="5dp"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/feedback"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/togivefeedback"
                                android:textSize="14sp"
                                android:textStyle="bold" />

                        </LinearLayout>

                        <View
                            android:id="@+id/feedbackdivider"
                            android:layout_width="match_parent"
                            android:layout_height="0.6dp"
                            android:layout_below="@id/feedback"
                            android:layout_marginTop="5dp"
                            android:background="@color/detailtextcolor" />
                        <!--feedback-->

                        <!-- help-->

                        <LinearLayout
                            android:id="@+id/help"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/feedbackdivider"
                            android:layout_margin="5dp"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/help"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/togethelp"
                                android:textSize="14sp"
                                android:textStyle="bold" />

                        </LinearLayout>

                        <View
                            android:id="@+id/helpdivider"
                            android:layout_width="match_parent"
                            android:layout_height="0.6dp"
                            android:layout_below="@id/help"
                            android:layout_marginTop="5dp"
                            android:background="@color/detailtextcolor" />

                        <!--help-->

                        <!--contact-->

                        <LinearLayout
                            android:id="@+id/contact"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/helpdivider"
                            android:layout_margin="5dp"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/contact"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/contacttodev"
                                android:textSize="14sp"
                                android:textStyle="bold" />

                        </LinearLayout>

                        <View
                            android:id="@+id/contactdivider"
                            android:layout_width="match_parent"
                            android:layout_height="0.6dp"
                            android:layout_below="@id/contact"
                            android:layout_marginTop="5dp"
                            android:background="@color/detailtextcolor" />

                        <!--contact-->

                        <!-- version-->

                        <LinearLayout
                            android:id="@+id/version"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/contactdivider"
                            android:layout_margin="5dp"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/version"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/v10"
                                android:textSize="14sp"
                                android:textStyle="bold" />

                        </LinearLayout>

                        <View
                            android:id="@+id/versiondivider"
                            android:layout_width="match_parent"
                            android:layout_height="0.6dp"
                            android:layout_below="@id/version"
                            android:layout_marginTop="5dp"
                            android:background="@color/detailtextcolor" />

                        <!-- version-->

                        <!--developer-->

                        <LinearLayout
                            android:id="@+id/developer"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/versiondivider"
                            android:layout_margin="5dp"
                            android:layout_marginTop="5dp"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:weightSum="2">

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/developer"
                                android:textColor="@color/detailtextcolor"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <me.myatminsoe.mdetect.MMTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_weight="1"
                                android:fontFamily="serif-monospace"
                                android:text="@string/zzz"
                                android:textSize="14sp"
                                android:textStyle="bold" />

                        </LinearLayout>

                        <!-- <View
                             android:id="@+id/versiondivider"
                             android:layout_width="match_parent"
                             android:layout_height="0.6dp"
                             android:layout_below="@id/version"
                             android:layout_marginTop="5dp"
                             android:background="@color/detailtextcolor" />


                         -->
                        <!--developer-->

                    </RelativeLayout>

                </android.support.v7.widget.CardView>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/card"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="2dp"
                    android:weightSum="2">

                    <RelativeLayout
                        android:id="@+id/torestore"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/time_bg_green">

                        <me.myatminsoe.mdetect.MMTextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_margin="5dp"
                            android:gravity="center"
                            android:padding="3dp"
                            android:text="@string/torestore"
                            android:textColor="@color/colorAccent"
                            android:textSize="14sp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/tobackup"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/time_bg_green">

                        <me.myatminsoe.mdetect.MMTextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_margin="5dp"
                            android:gravity="center"
                            android:padding="3dp"
                            android:text="@string/tobackup"
                            android:textColor="@color/colorAccent"
                            android:textSize="14sp" />

                    </RelativeLayout>

                </LinearLayout>

            </RelativeLayout>

        </RelativeLayout>

    </android.support.v4.widget.NestedScrollView>

</RelativeLayout>
